                    <option value="" disabled="disabled" selected="selected">Выберите время</option>
                    @foreach (var quest in Model.Where(t => t.QuestId == 2 && (t.DateTime >= ViewBag.Date) && t.IsReserved == false && t.DateTime.ToString("d") == ViewBag.Date.AddDays(i).ToString("d")).OrderBy(f => f.DateTime))
