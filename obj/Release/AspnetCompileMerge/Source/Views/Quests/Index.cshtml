                                        font-size:16px;
                                        color: white;
                                        font-family:'Exo 2',serif; font-weight:400;
                                        float:left;
                                        margin: 15px 0.2% 50px 0.2%; " id="btn-reserve-@quest.Id" class="btn-reserve-yet disabled">
                                        @quest.DateTime.ToString("t")

                                        </button>
                                    }


                                }
                                @{k++;
                                }
                                @if (m != 0)
                                {
                                    <div style="position:relative;width:100%">
                                        <div style="font-size:1vw;text-align:center;background-color:#ffbe00; height:5px; position:absolute; top:70px; left:@((n * 7.1 + 0.3).ToString("F").Replace(',', '.'))%; width:@(((k - n - 1) * 7.1 - 0.6).ToString("F").Replace(',', '.'))%;display:block;">
                                            <div style="margin-top:5px">
                                                @(m)р
                                            </div>
                                        </div>
                                    </div>
                                }
                                @*<div style="background-color:#ffbe00; height:5px; position:absolute; top:40px; left:@(n+40)px; width:@((k-n)*70)px"></div>*@
                            </div>
                                    }


                    </div>
                </div>
            </div>
        </div>

    </div>

</div>


