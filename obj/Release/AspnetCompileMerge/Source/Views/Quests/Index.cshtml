                <select id="Mobtimes_@(i)_3" class="form-control" style="display: none;border-color:#fccf21; max-width:none; height: 56px;font-size: 24px;font-weight:300;">
                    <option value="" disabled="disabled" selected="selected">Выберите время</option>
                    @foreach (var quest in Model.Where(t => t.QuestId == 3 && (t.DateTime >= ViewBag.Date) && t.IsReserved == false && t.DateTime.ToString("d") == ViewBag.Date.AddDays(i).ToString("d")).OrderBy(f => f.DateTime))
