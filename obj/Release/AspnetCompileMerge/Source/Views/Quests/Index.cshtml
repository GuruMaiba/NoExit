                    <option value="" disabled="disabled" selected="selected">Выберите время</option>
                    @foreach (var quest in Model.Where(t => t.QuestId == 1 && (t.DateTime >= ViewBag.Date) && t.IsReserved==false && t.DateTime.ToString("d") == ViewBag.Date.AddDays(i).ToString("d")).OrderBy(f => f.DateTime))
                    {
                        <option value="@quest.Id">@quest.DateTime.ToString("t") &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @(quest.Price)р</option>
                    }
                </select>

            }
        </div>
