        $('#btn-radio-2').addClass("hidden");
        $('#btn-radio-3').addClass("hidden");
        $('#btn-radio-4').addClass("hidden");
        $('#btn-radio-5').addClass("hidden");
        if(category==2)
        {
            $('#btn-radio-2').removeClass("hidden");
        }
        else if (category == 3) {
            changeCount(2);
            $('#btn-radio-2').removeClass("hidden");
            $('#btn-radio-3').removeClass("hidden");
            $('#btn-radio-4').removeClass("hidden");
        }
        else
        {
            $('#btn-radio-2').removeClass("hidden");
            $('#btn-radio-3').removeClass("hidden");
            $('#btn-radio-4').removeClass("hidden");
            $('#btn-radio-5').removeClass("hidden");
        };

        $('#div-1-'+category).removeClass("hidden");
        $('#div-2-'+category).removeClass("hidden");
        $('#div-3-'+category).removeClass("hidden");
        $('#Id').val(id);
        $('#myModal').modal('show');
    }
    function changeCount(id) {
        $('#Count').val(id);
        $('#btn-radio-2').removeClass("active");
        $('#btn-radio-3').removeClass("active");
        $('#btn-radio-4').removeClass("active");
        $('#btn-radio-5').removeClass("active");
        $('#btn-radio-'+id).addClass("active");
    }



