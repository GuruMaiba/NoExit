<script>
    window.onload = function () {

    };
    window.onresize = function () {
        var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
        var v = document.getElementsByTagName("video")[0];
        v.style.marginLeft = (width - v.width) / 2 + 'px';
        var v2 = document.getElementsByTagName("video")[1];
        v2.style.marginLeft = (width - v2.width) / 2 + 'px';
        var v3 = document.getElementsByClassName("imgQuest")[0];
        v3.style.marginLeft = (width - v3.width) / 2 + 'px';
        var v4 = document.getElementsByClassName("imgQuest")[1];
        v4.style.marginLeft = (width - v4.width) / 2 + 'px';
    };

    function vverh() {
        window.scrollBy(0, -15);
        if (window.pageYOffset > 0) { requestAnimationFrame(vverh); }
    }

    function show()
    {
        var currentItem = $("#carousel-example-generic .item.active" );
        var currentIndex = $('#carousel-example-generic .item').index(currentItem) + 1;
        if (currentIndex <= 2) {
            $('#btn-slider').css({
                opacity: 1
            }).fadeIn('slow');
        }
    }

    function slide(x)
    {
        $('#btn-slider').stop(true, false).fadeOut('fast');
        vverh();

        $("#carousel-example-generic").carousel(0);
        if ($("#quest1").hasClass("item")) {
            $("#quest1").removeClass("item");
            $("#quest1").addClass("hide");


        }
        if ($("#quest2").hasClass("item")) {
            $("#quest2").removeClass("item");
            $("#quest2").addClass("hide");


        }
        if ($("#quest3").hasClass("item")) {
            $("#quest3").removeClass("item");
            $("#quest3").addClass("hide");

        }

        $("#quest" + x).removeClass("hide");
        $("#quest" + x).addClass("item");

        $("#carousel-example-generic").carousel(2);
    }
    function prev() {
        $('#btn-slider').stop(true, false).fadeOut('fast');
        setTimeout(function () { show(); }, 3500);


        $("#carousel-example-generic").carousel("prev");
        var v = document.getElementsByTagName("video")[0];
        v.currentTime = 0;
        v.play();
        var v = document.getElementsByTagName("video")[1];
        v.currentTime = 0;
        v.play();
        if ($("#quest1").hasClass("item"))
        {
            $("#quest1").removeClass("item");
            $("#quest1").addClass("hide");

        }
        if ($("#quest2").hasClass("item")) {
            $("#quest2").removeClass("item");
            $("#quest2").addClass("hide");

        }
        if ($("#quest3").hasClass("item")) {
            $("#quest3").removeClass("item");
            $("#quest3").addClass("hide");

        }

    }
    function next() {
        $('#btn-slider').stop(true, false).fadeOut('fast');
        setTimeout(function () { show(); }, 3500);

        $("#carousel-example-generic").carousel("next");
        var v = document.getElementsByTagName("video")[0];
        v.currentTime = 0;
        v.play();
        var v = document.getElementsByTagName("video")[1];
        v.currentTime = 0;
        v.play();
        if ($("#quest1").hasClass("item")) {
            $("#quest1").removeClass("item");
            $("#quest1").addClass("hide");

        }
        if ($("#quest2").hasClass("item")) {
            $("#quest2").removeClass("item");
            $("#quest2").addClass("hide");

        }
        if ($("#quest3").hasClass("item")) {
            $("#quest3").removeClass("item");
            $("#quest3").addClass("hide");

        }

    }

    function beforeSlide() {

        $('#btn-slider').stop(true, false).fadeOut('fast');
        setTimeout(function () { show(); }, 3500);


        var v = document.getElementsByTagName("video")[0];
        v.currentTime = 0;
        v.play();
        var v = document.getElementsByTagName("video")[1];
        v.currentTime = 0;
        v.play();

    }
</script>

